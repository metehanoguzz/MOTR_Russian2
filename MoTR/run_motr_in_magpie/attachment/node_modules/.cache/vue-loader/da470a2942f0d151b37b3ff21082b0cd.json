default {\n  name: 'WaitForParticipants',\n  props: {\n    number: {\n      type: Number,\n      required: true\n    }\n  },\n  watch: {\n    ['$magpie.socket.active']() {\n      if (this.$magpie.socket.active.length === this.number) {\n        this.$emit('done');\n      }\n    }\n  },\n  mounted() {\n    if (this.$magpie.socket.active.length === this.number) {\n      this.$emit('done');\n    }\n  }\n};\n</script>\n"]}]}